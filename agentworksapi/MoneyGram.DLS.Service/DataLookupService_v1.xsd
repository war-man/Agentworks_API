		<xs:attribute name="sendTransactionDate" type="xs:dateTime" use="required" />
		<xs:attribute name="statusUpdateDate" type="xs:dateTime" use="required" />
		<xs:attribute name="product" type="xs:integer" use="required" />
		<xs:attribute name="status" type="xs:string" use="required" />
		<xs:attribute name="mgTranSessionId" type="xs:string" use="optional"/>
	</xs:complexType>
	
	<xs:element name="stoppedTransactionMessage">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="transaction" type="dls:StoppedTransactionDetail" />
			</xs:sequence>
			
			<xs:attribute name="version" type="xs:string" use="required" />
			<xs:attribute name="date" type="xs:dateTime" use="required" />
		</xs:complexType>
	</xs:element>

	<xs:complexType name="TransactionSummaryLookupRequest">
		<xs:complexContent>
			<xs:extension base="cmn:BaseServiceRequestMessage">
				<xs:sequence>
					<xs:choice>
						<xs:element name="senderPhoneNumber" type="dls:phoneNumberType" />
						<xs:group ref="dls:NamesSearchGroup" />
						<xs:element name="referenceNumber" type="dls:stringMax8Min8" />
					</xs:choice>
					<xs:element name="startDate" type="xs:date" minOccurs="0" />
					<xs:element name="endDate" type="xs:date" minOccurs="0" />
					<xs:element name="maximumTransactionCount" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1"></xs:minLength>
								<xs:maxLength value="3">
								</xs:maxLength>
								<xs:pattern value="\d+"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="NamesSearchGroup">
		<xs:sequence>
			<!-- receiverLastName is required but CCTS needs it to be optional for 
				client code generation -->
			<xs:element name="receiverLastName" type="dls:cnsmrNameType"
				minOccurs="0" />
			<xs:element name="receiverFirstName" type="dls:cnsmrNameType"
				minOccurs="0" />
			<xs:element name="senderLastName" type="dls:cnsmrNameType"
				minOccurs="0" />
			<xs:element name="senderFirstName" type="dls:cnsmrNameType"
				minOccurs="0" />
		</xs:sequence>
	</xs:group>
		
	<xs:complexType name="TransactionSummaryLookupResponse">
    	<xs:complexContent>
    		<xs:extension base="cmn:BaseServiceResponseMessage">
    			<xs:sequence>
    				<xs:element name="transactionSummary"
    					type="dls:TransactionSummary" minOccurs="0" maxOccurs="unbounded">
    				</xs:element>
    			</xs:sequence>
    		</xs:extension>
    	</xs:complexContent>
    </xs:complexType>

	<xs:complexType name="TransactionSummary">
		<xs:sequence>
			<xs:element name="senderFirstName" type="dls:cnsmrNameType"/>
			<xs:element name="senderLastName" type="dls:cnsmrNameType"/>
			<xs:element name="senderMiddleName" type="dls:cnsmrNameType"/>
			<xs:element name="senderMaternalLastName" type="dls:cnsmrNameType"/>
			<xs:element name="senderPhoneNumber" type="dls:phoneNumberType"/>
			<xs:element name="receiverFirstName" type="dls:cnsmrNameType"/>
			<xs:element name="receiverLastName" type="dls:cnsmrNameType"/>
			<xs:element name="receiverMiddleName" type="dls:cnsmrNameType"/>
			<xs:element name="receiverMaternalLastName" type="dls:cnsmrNameType"/>
			<xs:element name="referenceNumber" type="dls:stringMax8Min8"/>
			<xs:element name="productId" type="dls:stringMax10"/>
			<xs:element name="transactionID" type="dls:stringMax18"/>
			<xs:element name="transactionEventSequenceNumber" type="dls:integer3"/>
            <xs:element name="transactionStatus" type="dls:stringMax3"/>
            <xs:element name="transactionStatusUpdateDate" type="dls:dateStringType"/>
            <xs:element name="sendCurrrency" type="dls:stringMax3"/>
			<xs:element name="sendAmount" type="dls:amountType"/>
            <xs:element name="sendCountry" type="dls:isoCountryCodeType"/>
            <xs:element name="sendDate" type="dls:dateStringType"/>
            <xs:element name="receiveCurrency" type="dls:stringMax3"/>
			<xs:element name="receiveAmount" type="dls:amountType"/>
			<xs:element name="receiveCountry" type="dls:isoCountryCodeType"/>        </xs:sequence>
	</xs:complexType>
</xs:schema>